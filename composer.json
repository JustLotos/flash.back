{
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": ">=7.2",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "dama/doctrine-test-bundle": "6.3",
        "doctrine/doctrine-fixtures-bundle": "3.3",
        "doctrine/doctrine-migrations-bundle": "^3.1",
        "doctrine/migrations": "*",
        "gesdinet/jwt-refresh-token-bundle": "0.9.1",
        "jms/serializer": "3.4",
        "jms/serializer-bundle": "3.5",
        "lexik/jwt-authentication-bundle": "2.6",
        "mark-gerarts/automapper-plus-bundle": "1.2",
        "nelmio/api-doc-bundle": "3.5",
        "ramsey/uuid": "3.8",
        "sensio/framework-extra-bundle": "5.5",
        "sonata-project/admin-bundle": "3.76",
        "sonata-project/doctrine-orm-admin-bundle": "3.23",
        "squizlabs/php_codesniffer": "3.5.*",
        "symfony/apache-pack": "1.0",
        "symfony/asset": "4.4.*",
        "symfony/console": ">=4.4.29",
        "symfony/dotenv": "4.4.*",
        "symfony/flex": "^1.3.1",
        "symfony/form": "4.4.*",
        "symfony/framework-bundle": "4.4.41",
        "symfony/mailer": "4.4.*",
        "symfony/orm-pack": "1.0",
        "symfony/property-info": "4.*",
        "symfony/security-bundle": "4.*",
        "symfony/translation": "4.4.*",
        "symfony/twig-bundle": "4.4.*",
        "symfony/validator": "4.*",
        "symfony/webpack-encore-bundle": "1.7",
        "symfony/yaml": "4.4.*",
        "twig/extra-bundle": "^2.12|^3.0",
        "twig/twig": "*"
    },
    "require-dev": {
        "doctrine/coding-standard": ">=7.0.0",
        "fzaninotto/faker": "1.9",
        "symfony/browser-kit": "4.4.*",
        "symfony/css-selector": "4.4.*",
        "symfony/maker-bundle": "1.14",
        "symfony/monolog-bundle": "3.5",
        "symfony/phpunit-bridge": "^6.3",
        "symfony/stopwatch": "4.4.*",
        "symfony/web-profiler-bundle": "4.4.*"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true,
        "allow-plugins": {
            "symfony/flex": true,
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "4.4.*"
        }
    }
}
